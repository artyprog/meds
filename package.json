{
  "name": "meds",
  "version": "0.0.1",
  "description": "Manage a list of medications and get renewal alarms",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "nodemon --watch config --watch server server",
    "bundle": "NODE_ENV=production webpack --config config/webpack.config.js && npm run zip",
    "clean": "rm -r bundle",
    "zip": "rm public/*.gz && zopfli public/*",
    "server": "NODE_ENV=production node server"
  },
  "license": "AGPL-v3",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-register": "6.18.0",
    "babili-webpack-plugin": "0.0.7",
    "compression-webpack-plugin": "0.3.2",
    "magic-types": "github:magic/types",
    "node-zopfli": "2.0.2",
    "nodemon": "1.11.0",
    "svelte-loader": "1.1.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.14.0"
  },
  "dependencies": {
    "express": "4.14.0",
    "svelte": "1.6.1"
  }
}
